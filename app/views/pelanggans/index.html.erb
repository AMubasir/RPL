<p id="notice"><%= notice %></p>

<h1>Listing Pelanggans</h1>

<table>
  <thead>
    <tr>
      <th>Nama</th>
      <th>Alamat</th>
      <th>Jenis kelamin</th>
      <th>No telp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pelanggans.each do |pelanggan| %>
      <tr>
        <td><%= pelanggan.nama %></td>
        <td><%= pelanggan.alamat %></td>
        <td><%= pelanggan.jenis_kelamin %></td>
        <td><%= pelanggan.no_telp %></td>
        <td><%= link_to 'Show', pelanggan %></td>
        <td><%= link_to 'Edit', edit_pelanggan_path(pelanggan) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pelanggan', new_pelanggan_path %>
